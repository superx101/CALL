            "onStartDestroyBlock": {}
        },
        "area": {
            "start": {
                "x": null,
                "y": null,
                "z": null,
                "dimid": null
            },
            "end": {
                "x": null,
                "y": null,
                "z": null,
                "dimid": null
            }
        }
    }
}